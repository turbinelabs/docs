{
  "id": "/tbn/all-in-one/client",
  "cpus": 0.1,
  "mem": 64,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "turbinelabs/all-in-one-client:0.14.0",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "protocol": "tcp",
          "name": "http"
        }
      ],
      "forcePullImage": true
    }
  },
  "labels": {
    "tbn_cluster": "all-in-one-client",
    "stage": "prod"
  }
}
