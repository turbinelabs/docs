{
  "name": "rotor",
  "image": "turbinelabs/rotor:0.15.1",
  "cpu": 1,
  "memory": 128,
  "memoryReservation": 128,
  "environment": [
    {
      "name": "ROTOR_CMD",
      "value": "ecs"
    },
    {
      "name": "ROTOR_API_KEY",
      "value": "<your signed_token>"
    },
    {
      "name": "ROTOR_API_ZONE_NAME",
      "value": "<your TBN zone name>"
    },
    {
      "name": "ROTOR_ECS_AWS_ACCESS_KEY_ID",
      "value": "<your AWS access key>"
    },
    {
      "name": "ROTOR_ECS_AWS_REGION",
      "value": "<your AWS region>"
    },
    {
      "name": "ROTOR_ECS_AWS_SECRET_ACCESS_KEY",
      "value": "<your AWS secret access key>"
    },
    {
      "name": "ROTOR_ECS_CLUSTERS",
      "value": "<your cluster name>"
    }
  ]
}
